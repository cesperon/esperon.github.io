ener('scroll', this.updateScroll);\n  },\n  methods:{\n    updateScroll() {\n        console.log(this.scrollE);\n        this.scrollP = window.scrollY;\n        if(this.scrollP >= 1000){\n          this.scrollE = true;\n        }else{\n          this.scrollE = false;\n        }\n    },\n    scrollToDiv(div) {\n      console.log(div, 'from parent')\n      const element = document.getElementById(div);\n      element.scrollIntoView({behavior: 'smooth'});\n\n\n    },\n  },\n}\n</script>\n\n<style lang=\"scss\">\n@import 'assets/_shared.scss';\n// @import \"../assets/_shared.scss\";\n\n@media only screen and (max-width:500px) {\n  #div1{\n    margin-bottom:300px;\n  }\n  #div2{\n    margin-bottom:900px;\n  }\n  #main1{\n    display:block;\n  }\n}\n\n#app {\n\n  font-family:$default_font3;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n\n  #div2{\n    background:white;\n\n\n  }\n  #div1{\n    display:flex;\n    justify-content: center;\n    margin-top:400px;\n    background:rgb(238, 240,243);\n  \n  }\n  .scrollB{\n    margin-top:0;\n    width:250px;\n    height:70px;\n    border:2px solid black;\n    background:transparent;\n    font-family:$default_font2;\n    font-size:25px;\n    color:$blue;\n    box-shadow: 8px 8px 6px #888888;\n\n  }\n  .pBar{\n    width:50%;\n    height:55%;\n  }\n  .main1{\n    width:75%;\n    margin-top:100px;\n  }\n  .name{\n    color:$blue;\n  }\n  .hello{\n    margin-top:500px;\n    font-family:$default_font2;\n    font-size:40px;\n    font-weight:900;\n    color:black;  \n    margin-bottom:0;\n  }\n  .about{\n    height:50%;\n    width:35%;\n  }\n  .aboutChart{\n    width:600px;  \n    margin-right:200px;\n    margin-top:20px;\n  }\n\n  .scrollB:hover{\n      border:3px solid black;\n      font-size:28px;\n      width:300px;\n\n  }\n  .div {\n    height: 1200px;\n    border: 1px solid transparent;\n  }\n  .inLetter{\n    color:black;\n    font-size:40px;\n    font-fami