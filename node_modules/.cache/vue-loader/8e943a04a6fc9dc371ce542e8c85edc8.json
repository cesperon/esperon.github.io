dal = !this.openModal;\n    },\n    closePopUp() {\n      this.$emit('clicked', 'camp');\n    },\n    //handle counter for image gallery\n    increment(int){\n      if(this.imgIndex >= 0 && this.imgIndex <= 3){\n        if(this.imgIndex == 0 && int > 0 || this.imgIndex == 3 && int < 0 || this.imgIndex > 0 && this.imgIndex < 3)\n        {\n            this.imgIndex += int;\n        }\n\n      }\n      console.log(this.imgIndex);\n    },\n  },\n\n};\n</script>\n<style lang=\"scss\">\n.popup{\n  position: fixed;\n  top: 0;\n  left: 0;\n  right: 0;\n  bottom: 0;\n  z-index: 99;\n  background-color: rgba(0,0,0,0.2);\n  display: flex;\n  align-items:center;\n  justify-content: center;\n\n\n  .popup-inner {\n    background: white;\n    // padding: 32px;\n    width:700px;\n    height:700px;\n    border-radius:10px;\n    padding-top:0;\n    box-shadow: 8px 8px 6px #888888;\n\n\n  }\n  .changeImgRight{\n    position:absolute;\n    top:0;\n    right:0;\n    margin-top:120px;\n    margin-right:10px;\n  }\n  .changeImgLeft{\n    position:absolute;\n    top:0;\n    left:0;\n    margin-top:120px;\n    margin-left:10px;\n  }\n  .stack-img{\n    height:20px;\n  }\n\n\n}\n</style>"]}]}